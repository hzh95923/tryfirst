			<script type="text/javascript">
				var PromotionTime = {
					timer: null,
					init: function() {
						// 夏令时提前一小时
						var totalSecond = this.totalTime("2017/04/24 23:59:59 GMT-0700", $(
							"#hidTotalSecond").val());
						if(totalSecond <= 0)
							return;
						this.setTime(totalSecond);
					},
					totalTime: function(date, sysdate) {
						var data = new Date(date);
						var longTime = data.getTime();
						var totalSecond = parseInt((longTime - sysdate) / 1000);
						return totalSecond;
					},
					setTime: function(totalSecond) {
						/*var dayText = "days";*/
						var day = Math.floor(totalSecond / (60 * 60 * 24));
						var hour = Math.floor(totalSecond / 3600) - (day * 24);
						var minute = Math.floor(totalSecond / 60) - (day * 24 * 60) -
							(hour * 60);
						var second = Math.floor(totalSecond) - (day * 24 * 60 * 60) -
							(hour * 60 * 60) - (minute * 60);
						var timeOver = parseInt(day) < 1 && parseInt(hour) < 1 &&
							parseInt(minute) < 1 && parseInt(second) < 1;

						if(second < 10) {
							$("#secondLeft").html("0");
							$("#secondRight").html(second);
						} else {
							var strS = second.toString(),
								s1 = strS.charAt(0),
								s2 = strS
								.charAt(1);
							$("#secondLeft").html(s1);
							$("#secondRight").html(s2);
						}

						if(minute < 10) {
							$("#minuteLeft").html("0");
							$("#minuteRight").html(minute);

						} else {
							var strM = minute.toString(),
								m1 = strM.charAt(0),
								m2 = strM
								.charAt(1);
							$("#minuteLeft").html(m1);
							$("#minuteRight").html(m2);
						}

						if(hour < 10) {
							$("#hourLeft").html("0");
							$("#hourRight").html(hour);
						} else {
							var strH = hour.toString(),
								h1 = strH.charAt(0),
								h2 = strH
								.charAt(1);
							$("#hourLeft").html(h1);
							$("#hourRight").html(h2);
						}

						if(day < 10) {
							$("#dayLeft").html("0");
							$("#dayRight").html(day);
						} else {
							var strD = day.toString(),
								d1 = strD.charAt(0),
								d2 = strD
								.charAt(1);
							$("#dayLeft").html(d1);
							$("#dayRight").html(d2);
						}

						/*if (day <= 1) {
						    dayText = "day";
						}
						$("#dayText").html(dayText);*/

						PromotionTime.timer = setTimeout(function() {
							if(timeOver) {
								$(".promotion_time").hide();
							} else {
								PromotionTime.setTime(totalSecond - 1);
							}
						}, 1000);
					}
				};
				$(function() {
					PromotionTime.init();
				});
			</script>
			<script>
				//放假通知
				(function($) {
					var docCookies = {
						getItem: function(sKey) {
							return decodeURIComponent(document.cookie.replace(new RegExp(
									"(?:(?:^|.*;)\\s*" +
									encodeURIComponent(sKey).replace(
										/[\-\.\+\*]/g, "\$&") +
									"\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) ||
								null;
						},
						setItem: function(sKey, sValue, vEnd, sPath, sDomain, bSecure) {
							if(!sKey ||
								/^(?:expires|max\-age|path|domain|secure)$/i
								.test(sKey)) {
								return false;
							}
							var sExpires = "";
							if(vEnd) {
								switch(vEnd.constructor) {
									case Number:
										sExpires = vEnd === Infinity ? "; expires=Fri, 31 Dec 9999 23:59:59 GMT" :
											"; max-age=" + vEnd;
										break;
									case String:
										sExpires = "; expires=" + vEnd;
										break;
									case Date:
										sExpires = "; expires=" + vEnd.toUTCString();
										break;
								}
							}
							document.cookie = encodeURIComponent(sKey) + "=" +
								encodeURIComponent(sValue) + sExpires +
								(sDomain ? "; domain=" + sDomain : "") +
								(sPath ? "; path=" + sPath : "") +
								(bSecure ? "; secure" : "");
							return true;
						},
						removeItem: function(sKey, sPath, sDomain) {
							if(!sKey || !this.hasItem(sKey)) {
								return false;
							}
							document.cookie = encodeURIComponent(sKey) +
								"=; expires=Thu, 01 Jan 1970 00:00:00 GMT" +
								(sDomain ? "; domain=" + sDomain : "") +
								(sPath ? "; path=" + sPath : "");
							return true;
						},
						hasItem: function(sKey) {
							return(new RegExp("(?:^|;\\s*)" +
								encodeURIComponent(sKey).replace(/[\-\.\+\*]/g,
									"\$&") + "\\s*\\=")).test(document.cookie);
						},
						keys: function() {
							var aKeys = document.cookie
								.replace(
									/((?:^|\s*;)[^\=]+)(?=;|$)|^\s*|\s*(?:\=[^;]*)?(?:\1|$)/g,
									"").split(/\s*(?:\=[^;]*)?;\s*/);
							for(var nIdx = 0; nIdx < aKeys.length; nIdx++) {
								aKeys[nIdx] = decodeURIComponent(aKeys[nIdx]);
							}
							return aKeys;
						}
					};
					var $closeBtn = $("#closeBtn");
					if(!docCookies.hasItem('tips')) {
						$closeBtn.closest(".noticeTip").show();
					}
					$closeBtn.click(function() {
						$(this).closest(".noticeTip").hide();
						docCookies.setItem('tips', 'holiday notice', Infinity, '/');
					});

				})(jQuery);
